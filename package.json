{
  "name": "angular-boiler-plate",
  "version": "0.0.0",
  "scripts": {
    "config": "ts-node ./scripts/setenv.ts",
    "postinstall": "npm run prepare",
    "prepare": "husky install",
    "ng": "ng",
    "start": "npm run config && node --max-old-space-size=8192 node_modules/@angular/cli/bin/ng serve",
    "build": "node --max-old-space-size=8192 node_modules/@angular/cli/bin/ng build",
    "docker:build": "docker build -t oasis-frontend:latest .",
    "pretest": "npm run lint",
    "test": "ng test --no-watch",
    "test:debug": "npm run lint && ng test",
    "prelint": "npm run build",
    "lint": "ng lint && npm run prettier:check",
    "watch": "ng build --watch --configuration development",
    "prettier:cli": "prettier \"**/*.ts\" \"**/*.js\" \"**/*.html\"",
    "prettier:check": "npm run prettier:cli -- --check",
    "prettier:fix": "npm run prettier:cli -- --write",
    "release": "standard-version",
    "preanalyze": "npm run build -- --stats-json --configuration=prodstats",
    "analyze": "webpack-bundle-analyzer dist/biz-book-ui/stats.json",
    "compress": "gzipper compress ./dist --gzip --gzip-level 9 --include js,html,css,ttf,woff,woff2,eot",
    "compress:instrument": "gzipper compress ./instrumented --gzip --gzip-level 9 --include js,html,css,ttf,woff,woff2,eot",
    "instrument": "NODE_OPTIONS=--max-old-space-size=16384 npx nyc instrument ./dist ./instrumented --delete --complete-copy --exclude-after-remap=false",
    "newbuild": "ng build --named-chunks --verbose --build-optimizer=false --source-map"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@nebular/auth": "^10.0.0",
    "@nebular/date-fns": "^10.0.0",
    "@nebular/eva-icons": "^10.0.0",
    "@nebular/theme": "^10.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "eva-icons": "^1.1.3",
    "lodash": "^4.17.21",
    "ngx-logger": "^5.0.11",
    "ngx-permissions": "^14.0.0",
    "ngx-webstorage-service": "^5.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular-eslint/builder": "^14.4.0",
    "@angular-eslint/eslint-plugin": "^14.4.0",
    "@angular-eslint/eslint-plugin-template": "^14.4.0",
    "@angular-eslint/schematics": "^14.4.0",
    "@angular-eslint/template-parser": "^14.4.0",
    "@angular/cli": "~14.2.10",
    "@angular/compiler-cli": "^14.0.0",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@schematics/angular": "^15.0.4",
    "@types/jasmine": "~4.3.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "commitizen": "^4.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "cz-customizable-ghooks": "^2.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.2",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "^2.8.1",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}